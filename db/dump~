CREATE TABLE "journals" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "group" varchar(255), "activity" text, "date" date, "hours" integer, "minutes" integer, "user_id" integer, "project_id" integer, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
