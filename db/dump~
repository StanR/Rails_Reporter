CREATE TABLE "projects" ("id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, "title" varchar(255), "description" text, "platform" varchar(255), "tools" text, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
